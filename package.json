{
  "name": "vigne",
  "version": "1.0.0",
  "description": "Vigne",
  "main": "index.js",
  "scripts": {
    "build_js": "babel javascripts --out-file public/script.js --source-maps",
    "build_css": "cleancss -o public/style.css css/* --source-map",
    "build": "rm -rf public && mkdir public && npm run build_js && npm run build_css",
    "dev": "npm run build && DEBUG=vigne nodemon index.js",
    "start": "npm run build && node -r babel-register index.js"
  },
  "keywords": [],
  "author": "NerOcrO",
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.8.6"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.4.3",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.3",
    "clean-css-cli": "^4.2.0",
    "compression": "^1.7.3",
    "debug": "^3.1.0",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "helmet": "^3.13.0",
    "moment": "^2.22.2",
    "mysql": "^2.16.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "nodemon": "^1.18.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NerOcrO/vigne"
  }
}
